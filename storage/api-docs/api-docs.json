                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request"
                    }
                }
            }
        },
        "/api/blog-categories/{id}": {
            "get": {
                "tags": [
                    "Blog Categories"
                ],
                "summary": "Get details of a Blog Category",
                "operationId": "08fed79fd7d2cc9ddb83253e83ee32e7",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the blog category",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "data": {
                                            "properties": {
                                                "status": {
                                                    "type": "string",
                                                    "example": "enable"
                                                },
                                                "priority": {
                                                    "type": "integer",
                                                    "example": 73091
                                                },
                                                "description": {
                                                    "type": "string",
                                                    "example": "Corrupti optio aliquam maiores facilis corporis fuga."
                                                },
                                                "blog_category": {
                                                    "type": "string",
                                                    "example": "Eos"
                                                },
                                                "time_of_creation": {
                                                    "type": "string",
                                                    "example": "1 day ago"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Blog category not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "error"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "No query results for model [BlogCategory]."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request"
                    }
                }
            }
        },
