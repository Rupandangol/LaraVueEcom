                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "status": {
                                                        "type": "string",
                                                        "example": "enable"
                                                    },
                                                    "priority": {
                                                        "type": "integer",
                                                        "example": 73091
                                                    },
                                                    "description": {
                                                        "type": "string",
                                                        "example": "Corrupti optio aliquam maiores facilis corporis fuga."
                                                    },
                                                    "blog_category": {
                                                        "type": "string",
                                                        "example": "Eos"
                                                    },
                                                    "time_of_creation": {
                                                        "type": "string",
                                                        "example": "9 seconds ago"
                                                    },
                                                    "random": {
                                                        "type": "string",
                                                        "example": "0hSp5ALxdTspNyet"
                                                    },
                                                    "blog_count": {
                                                        "type": "integer",
                                                        "example": 0
